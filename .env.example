FEATHERLESS_API_KEY=
FEATHERLESS_BASE_URL=https://api.featherless.ai/v1
DEFAULT_MODEL=moonshotai/Kimi-K2-Instruct
REQUEST_TIMEOUT_SECONDS=45
REQUEST_MIN_INTERVAL_SECONDS=1.0
REQUEST_MAX_RETRIES=6
REQUEST_RETRY_BACKOFF_BASE_SECONDS=1.0
REQUEST_RETRY_MAX_SLEEP_SECONDS=20.0
AUTH_ENABLED=true
AUTH_JWT_HS256_SECRET=
AUTH_JWT_ISSUER=
AUTH_JWT_AUDIENCE=
AUTH_ROLES_CLAIM=roles
AUTH_TENANT_CLAIM=tenant_id
AUTH_DEFAULT_TENANT_ID=default
AUTH_LEGACY_DEFAULT_TENANT_ID=legacy
RBAC_ENABLED=true
JOB_STORE_BACKEND=file
# Examples:
# sqlite: JOB_STORE_BACKEND=sql and JOB_STORE_DSN=sqlite:///absolute/path/jobs.db
# postgres: JOB_STORE_BACKEND=sql and JOB_STORE_DSN=postgresql://user:pass@host:5432/dbname
JOB_STORE_DSN=
RUN_EXECUTOR_MODE=inline

# Optional: if set, manifest.json includes an HMAC signature.
# Keep this secret and do not commit it.
VENDOR_RTP_MANIFEST_HMAC_KEY=
